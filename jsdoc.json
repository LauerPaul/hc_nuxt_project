{
  "plugins": [
    "node_modules/jsdoc-vue-component",
    "plugins/summarize",
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "presets": [ "es2015" ],
    "plugins": [ "transform-async-to-generator" ]
  },
  "jsdoc-vue-component": {
    "log": true
  },
  "markdown": {
    "tags": ["author", "classdesc", "description", "param", "property", "returns", "see", "throws", "vue"]
  },
  "recurseDepth": 10,
  "source": {
    "include": ["api", "components", "layouts", "middleware", "pages", "plugins", "store", "services"],
    "exclude": [],
    "includePattern": ".+\\.(js|vue)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "recurse": true,
    // "readme": "README.md"
  }
}